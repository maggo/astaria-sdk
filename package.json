{
  "name": "@astariaxyz/sdk",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"yarn build:cjs\" \"yarn build:esm\"",
    "build:clean": "rimraf dist && yarn build",
    "build:cjs": "tsc -p tsconfig.cjs.json && node postbuild-cjs.mjs",
    "build:esm": "tsc -p tsconfig.esm.json && node postbuild-esm.mjs",
    "test": "jest -c jest.config.js --silent --maxWorkers=50%",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "lint:fix": "yarn format:fix && yarn lint:ts:fix && yarn type-check",
    "lint:ts:fix": "eslint --quiet --fix .",
    "lint:ts": "eslint --quiet .",
    "lint": "yarn format && yarn lint:ts && yarn type-check",
    "type-check:watch": "tsc --watch --noEmit",
    "type-check": "tsc --noEmit",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "json-stringify-deterministic": "^1.0.8",
    "merkletreejs": "^0.3.9",
    "papaparse": "^5.3.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/papaparse": "^5.3.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "abitype": "^0.9.8",
    "concurrently": "^8.2.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "husky": "^8.0.2",
    "ipfs-only-hash": "^4.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "semantic-release": "^21.1.1",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "viem": "^1.9.3",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "viem": ">1.x.x",
    "zod": ">3.21.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astariaxyz/astaria-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/cjs",
    "dist/esm",
    "!dist/**/*.tsbuildinfo",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "sideEffects": false,
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ]
}
