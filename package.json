{
  "name": "@astariaxyz/sdk",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watchAll",
    "test:coverage": "tsdx test --coverage",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "lint:fix": "yarn format:fix && yarn lint:ts:fix && yarn type-check",
    "lint:ts:fix": "eslint --quiet --fix .",
    "lint:ts": "eslint --quiet .",
    "lint": "yarn format && yarn lint:ts && yarn type-check",
    "type-check:watch": "tsc --watch --noEmit",
    "type-check": "tsc --noEmit",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "axios": "^1.2.2",
    "caniuse-lite": "^1.0.30001519",
    "ipfs-only-hash": "^4.0.0",
    "json-stringify-deterministic": "^1.0.8",
    "merkletreejs": "^0.3.9",
    "papaparse": "^5.3.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/papaparse": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "abitype": "^0.9.6",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-security": "^1.7.1",
    "ethereum-waffle": "^4.0.10",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "semantic-release": "^19.0.5",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^5.2.2",
    "viem": "^1.5.4",
    "zod": "^3.21.4"
  },
  "resolutions": {
    "@babel/core": "^7.12.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3"
  },
  "peerDependencies": {
    "viem": ">1.x.x",
    "zod": ">3.21.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astariaxyz/astaria-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/sdk.esm.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ]
