{
  "name": "@astariaxyz/sdk",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watchAll",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write !src/contracts/**/* !src/test/__mocks__/**/* !abi !dist .",
    "typechain:compile": "typechain --target ethers-v5 --out-dir './src/contracts' './abi/*.json'",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "ethers": "^5.7.2",
    "ipfs-only-hash": "^4.0.0",
    "json-stringify-deterministic": "^1.0.8",
    "merkletreejs": "^0.3.9",
    "papaparse": "^5.3.2",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/papaparse": "^5.3.7",
    "ganache": "^7.7.2",
    "husky": "^8.0.2",
    "semantic-release": "^19.0.5",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typechain": "^8.1.1"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "@babel/core": "^7.12.3",
    "prettier": "^2.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astariaxyz/astaria-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/sdk.esm.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsdx lint",
      "eslint --cache --fix",
      "yarn format"
    ]
  }
}
